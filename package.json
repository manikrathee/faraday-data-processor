{
  "name": "faraday-data-processor",
  "version": "1.0.0",
  "description": "High-performance data processing pipeline for health and fitness data normalization",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "process": "node src/index.js process",
    "process-db": "node src/index.js process --database",
    "process-mysql": "node src/index.js process --database --db-type mysql",
    "analyze": "node src/index.js analyze",
    "test": "node src/test.js",
    "test-jest": "jest",
    "test-validation": "node src/quick-validation.js",
    "test-enterprise": "node src/comprehensive-validation.js",
    "lint": "eslint src/",
    "dev": "nodemon src/index.js",
    "db:create": "node src/index.js database --create",
    "db:stats": "node src/index.js database --stats",
    "db:reset": "node src/index.js database --reset",
    "db:vacuum": "node src/index.js database --vacuum"
  },
  "bin": {
    "faraday-processor": "./src/index.js"
  },
  "keywords": ["health", "fitness", "data-processing", "json", "normalization", "enterprise"],
  "author": "Manik Rathee",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^8.7.0",
    "commander": "^11.0.0",
    "csv-parser": "^3.0.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mysql2": "^3.15.3",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.50.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}